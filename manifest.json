{
  "manifest_version": 2,
  "name": "CSSVacuum",
  "version": "0.1.0",
  "description": "Optimize your critical rendering path",
  "homepage_url": "https://github.com/ndreckshage/CSSVacuum",
  "icons": {
    "16": "/images/16.png",
    "48": "/images/48.png",
    "128": "/images/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/lib/jquery-2.0.3.min.js",
        "/lib/raphael-2.1.2.min.js",
        "/cssvacuum.js"
      ]
    }
  ]
}