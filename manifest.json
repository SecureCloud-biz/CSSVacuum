{
  "name": "CSSvacuum",
  "version": "0.1.0",
  "description": "Optimize your critical rendering path",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/lib/jquery-2.0.3.min.js",
        "/lib/raphael-2.1.2.min.js",
        "/cssvacuum.js"
      ]
    }
  ],
  "manifest_version": 2
}